{{#each events}}
export interface {{this.name}}Args {
  {{#each inputs}}
  {{name}}: {{#parameterType type}}{{/parameterType}};
  {{/each}}
}
{{/each}}

export const enum {{contractName}}Events {
  {{#each events}}
  {{this.name}} = "{{this.name}}",
  {{/each}}
}

{{#each events}}
export type {{this.name}}Log = DecodedLogEntry<{{this.name}}Args, {{../contractName}}Events.{{this.name}}>;
export type {{this.name}}LogEvent = DecodedLogEntryEvent<{{this.name}}Args, {{../contractName}}Events.{{this.name}}>;
{{/each}}

export type {{contractName}}Logs =
{{#each events}}
  {{name}}Log{{#unless @last}} |{{/unless}}{{^unless @last}};{{/unless}}
{{/each}}

export type {{contractName}}LogEvents =
{{#each events}}
  {{name}}LogEvent{{#unless @last}} |{{/unless}}{{^unless @last}};{{/unless}}
{{/each}}
public {{this.name}} = {
  async sendTransactionAsync(
    {{> typed_inputs inputs=inputs}}
    {{#this.payable}}
    txData: TxDataPayble = {},
    {{/this.payable}}
    {{^this.payable}}
    txData: TxData = {},
    {{/this.payable}}
  ): Promise<MultisigProxyTransaction> {
    if (!isDefined(this.multisig)) {
      return this.emptyProxyTransaction(
        await this.instance.{{this.name}}.sendTransactionAsync(
          {{> params inputs=inputs}}
          txData,
        ),
      );
    }
    const raw = await this.instance.{{this.name}}.getRaw(
      {{> params inputs=inputs}}
      {...txData, gas: 0},
    );
    return this.createProxyTransaction(
      await this.multisig.submitTransaction(
        this.instance.address,
        {{#this.payable}}
        txData.value,
        {{/this.payable}}
        {{^this.payable}}
        new BigNumber(0),
        {{/this.payable}}
        raw.data,
      ),
    );
  },
};

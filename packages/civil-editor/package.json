{
  "name": "civil-editor",
  "version": "1.0.0",
  "description": "editor for civil formatted articles",
  "main": "dist/index.js",
  "license": "MIT",
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "build"
    ],
    "testRegex": "(src/__test__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "setupTestFrameworkScriptFile": "<rootDir>src/__test__/setupTests.js",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "scripts": {
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "jest"
  },
  "devDependencies": {
    "@storybook/addon-storyshots": "^3.3.14",
    "@storybook/react": "^3.3.14",
    "@types/jest": "^22.1.4",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.39",
    "@types/react-dom": "^16.0.4",
    "@types/react-test-renderer": "^16.0.1",
    "@types/storybook__react": "^3.0.7",
    "awesome-typescript-loader": "^3.5.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "core-js": "^2.5.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.4.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "strip-ansi": "^4.0.0",
    "ts-jest": "^22.4.0",
    "ts-loader": "^3",
    "typescript": "^2.7.2",
    "webpack": "^3",
    "webpack-hot-middleware": "^2.21.1"
  },
  "peerDependencies": {
    "immutable": "^3.8.2",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "dependencies": {
    "@types/enzyme": "^3.1.9",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "slate": "^0.33.0",
    "slate-react": "^0.12.0",
    "styled-components": "^3.1.6"
  }
}

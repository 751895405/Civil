export enum {{contractName}}Events {
  {{#each events}}
  {{this.name}} = "{{this.name}}",
  {{/each}}
}

export type {{contractName}}EventArgs =
  {{#each events}}
    {{name}}Args{{#unless @last}} |{{/unless}}{{^unless @last}};{{/unless}}
  {{/each}}

{{#each events}}
export interface {{this.name}}Args {
  {{#each inputs}}
  {{name}}: {{#parameterType type}}{{/parameterType}};
  {{/each}}
}
{{/each}}